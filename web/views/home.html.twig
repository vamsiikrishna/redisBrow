{% extends layout %}
{% block body %}

    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">redisBrow</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="/">Server Status</a></li>
                    <li><a href="/keys">Keys</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>


   <div class="col-md-12">

       <div class="col-md-8">
        <p><strong>{{ dbsize }} Keys available.</strong></p>
        <p>Used Memory: {{ info.Memory.used_memory_human }}, peak: {{ info.Memory.used_memory_peak_human }}</p>


           <table class="table table-condensed table-hover">
               <thead>
               <tr>
                   <th>Key</th>
                   <th>Value</th>
               </tr>
               </thead>
               <tbody>


               {% for key, value in info.Server %}
                   <tr>
                   <td>{{ key }}</td>
                   <td>{{ value }}</td>
                </tr>
               {% endfor %}


               </tbody>
           </table>




















       </div>

       <div class="col-md-4">

       </div>

    </div>

{% endblock body %}